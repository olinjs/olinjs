<h2>Welcome to Twoter</h2>
<br></br>
<form id = 'logInButton' action='login' method='GET'>
    <input type='submit' class='login' value='Log In'>
</form>


<br></br>

<div id='allTwotes'>
    {{#each alltwotes}}
        <div id='{{this.id}}-twote'>
            <li>{{this.user}} said: "{{this.text}}"</li>
        </div>
    {{/each}}
</div>

<div id='allUsers'>
    <h3>User List</h3>
    {{#each allusers}}
    <div id='user-{{this.name}}' class='user'>
        <p>User {{this.name}}</p>
    </div>
    {{/each}}
</div>

