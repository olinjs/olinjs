<body>
	<div class="container">
		<div class="row">
			<div class="col-lg-12"><h2>{{page-header}} <small> {{page-subheader}}</small></h2></div>
		</div>

		<div class="starter-template">
		  <table class="table table-hover table-striped">
		  	{{#each data}}
		    <tr class="row">
			  <td>
			    <div class='col-xs-1 text-left'>{{age}}</div>
			    <div class='col-xs-5 text-left'>{{name}}</div>
			    <div class='col-xs-6 text-right'>{{#each color}} &nbsp&nbsp&nbsp{{this}} {{/each}}</div>
			  </td>
			</tr>
			{{/each}}
		  </table>
		</div>
	</div>
</body>